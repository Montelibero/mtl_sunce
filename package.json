{
  "name": "org.montelibero.sunce",
  "displayName": "Sunce Wallet",
  "version": "1.0.1",
  "description": "Wallet for the Stellar payment network by Montelibero.",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Montelibero Sunce Team",
    "email": "sunce@montelibero.org"
  },
  "homepage": "https://sunce.montelibero.org/",
  "repository": "github:montelibero/mtl_sunce",
  "main": "./electron/lib/index.prod.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint:fix": "eslint --fix '{src,stories}/**/*.{ts,tsx}'",
    "prettier": "prettier --write '{src,stories}/**/*.{ts,tsx}' *.md electron/src/*.ts",
    "storybook": "storybook dev -p 9001 -c .storybook",
    "storybook:build": "storybook build -c .storybook",
    "test": "tsc --noEmit && eslint",
    "clean:dist": "rimraf dist/*",
    "clean:electron:ts": "rimraf electron/lib/*",
    "prebuild:electron:bundle": "cross-env NODE_ENV=production parcel build ./src/index.prod.njk --detailed-report --no-cache --no-source-maps --public-url=./",
    "prebuild:electron:ts": "npm run clean:electron:ts && cd electron && tsc",
    "prebuild:electron:ts:watch": "npm run clean:electron:ts && cd electron && tsc -w",
    "prebuild:electron": "run-s prebuild:electron:ts clean:dist prebuild:electron:bundle",
    "build:linux": "PLATFORM=linux npm run prebuild:electron && electron-builder -p always --linux --x64 --config ./electron-build.yml",
    "build:mac": "PLATFORM=darwin npm run prebuild:electron && electron-builder -p always --mac --x64 --arm64 --config ./electron-build.yml",
    "build:mac:signed": "source ./scripts/signing-env.sh mac && NOTARIZE=true npm run build:mac -- -c.forceCodeSigning=true",
    "build:win": "export arch=x64|| set arch=x64 && npm run prebuild:electron && electron-builder -p always --win --x64 --config ./electron-build.yml",
    "build:win:signed": "source ./scripts/signing-env.sh win && npm run build:win -- -c.forceCodeSigning=true",
    "dev": "run-p prebuild:electron:ts:watch dev:bundle dev:app:delayed",
    "dev:app": "cross-env NODE_ENV=development TS_NODE_PROJECT=./electron/tsconfig.json run-electron -r ts-node/register/transpile-only ./electron/src/index.dev",
    "dev:app:delayed": "wait-on -t 2000 && run-s dev:app",
    "dev:bundle": "cross-env NODE_ENV=development parcel watch ./src/index.dev.njk --hmr-hostname localhost --public-url=./"
  },
  "lint-staged": {
    "ignore": [
      "**/*.yml",
      "**/*.svg",
      "**/*.png",
      "**/*.template",
      "**/*.njk"
    ],
    "linters": {
      "*.{json,yml,md,njk,ts,tsx,js,jsx}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@stellarguard/stellar-uri": "^2.0.0",
    "electron-context-menu": "^0.15.0",
    "electron-debug": "^3.0.1",
    "electron-is-dev": "^1.1.0",
